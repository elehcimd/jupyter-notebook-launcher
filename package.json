{
  "name": "jupyter-notebook-launcher",
  "version": "1.0.0",
  "description": "Manage Jupyter Notebook servers from menu bar (macOS-only)",
  "main": "app/app.js",
  "scripts": {
    "build": "babel src/ --out-dir app/",
    "start": "npm run build && electron .",
    "publish": "build --mac -p always",
    "dist": "npm run build && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kysely/jupyter-notebook-launcher.git"
  },
  "keywords": [
    "jupyter",
    "notebook",
    "server",
    "manage",
    "launch",
    "spawn",
    "kill",
    "multiple",
    "menubar",
    "cwd"
  ],
  "author": "Radek Kysely",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kysely/jupyter-notebook-launcher/issues"
  },
  "homepage": "https://github.com/kysely/jupyter-notebook-launcher#readme",
  "build": {
    "appId": "com.github.kysely.jupyter-notebook-launcher",
    "productName": "Jupter Notebook Launcher",
    "files": [
      "!src${/*}",
      "!static/*.gif"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    }
  },
  "publish": [
    {
      "provider": "github",
      "owner": "kysely",
      "repo": "jupyter-notebook-launcher"
    }
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "electron": "^1.7.4",
    "electron-builder": "^19.16.1"
  },
  "dependencies": {
    "auto-launch": "^5.0.1",
    "electron-store": "^1.2.0",
    "electron-updater": "^2.7.1",
    "jimp": "^0.2.28",
    "locate-executable": "^1.1.0",
    "mkdirp": "^0.5.1"
  }
}
